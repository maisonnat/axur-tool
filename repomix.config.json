{
    "output": {
        "style": "xml",
        "parsableStyle": true,
        "compress": true,
        "removeComments": true,
        "headerText": "Antigravity Project Context - Generated by Repomix",
        "instructionFilePath": ".repomix-instruction.md",
        "fileSummary": true,
        "directoryStructure": true,
        "topFilesLength": 10,
        "git": {
            "sortByChanges": true,
            "includeDiffs": true,
            "includeLogs": false
        }
    },
    "ignore": {
        "useGitignore": true,
        "useDefaultPatterns": true,
        "customPatterns": [
            "**/*.lock",
            "**/target/**",
            "**/node_modules/**",
            "**/dist/**",
            "**/.git/**",
            "**/*.png",
            "**/*.jpg",
            "**/*.ico",
            "**/*.svg"
        ]
    },
    "security": {
        "enableSecurityCheck": true
    }
}